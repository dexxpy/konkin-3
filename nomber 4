#include <iostream>
#include <vector>
using namespace std;

int main() 
{
    
    vector<int> numbers = { 3, -1, 4, -1, 5, -9, 2, -6 };

    for (int number : numbers) {
        if (number >= 0) {
            cout << number << " "; //  положительные числа
        }
    }

    for (int number : numbers) {
        if (number < 0) {
            cout << number << " "; //  отрицательные числа 
        }
    }

   cout << endl;

    return 0;
}
